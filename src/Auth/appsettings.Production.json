{
  "Urls": {
    "PersonalAssistant": "https://personalassistant.site",
    "ToDoAssistant": "https://todo.personalassistant.site",
    "CookingAssistant": "https://cooking.personalassistant.site",
    "Accountant": "https://accountant.personalassistant.site",
    "IssuerUri": "https://personalassistant.site"
  },
  "SessionKeysDirectory": "session-keys/",
  "Certificate": {
    "Directory": "certs/"
  },
  "Serilog": {
    "MinimumLevel": "Error",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "log.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "ApplicationInsights": {
    "EnableAdaptiveSampling": false,
    "EnablePerformanceCounterCollectionModule": false
  }
}