{
  "Urls": {
    "Authority": "https://personalassistant.site",
    "ToDoAssistant": "https://todo.personalassistant.site",
    "CookingAssistant": "https://cooking.personalassistant.site",
    "Accountant": "https://accountant.personalassistant.site"
  },
  "Currencies": {
    "RatesFilePath": "storage/currencyrates.json"
  },
  "Serilog": {
    "MinimumLevel": "Error",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "log.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "ApplicationInsights": {
    "EnableAdaptiveSampling": false,
    "EnablePerformanceCounterCollectionModule": false
  }
}