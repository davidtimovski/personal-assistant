{
  "Urls": {
    "ToDoAssistant": "https://todo.personalassistant.site",
    "CookingAssistant": "https://cooking.personalassistant.site",
    "Accountant": "https://accountant.personalassistant.site",
    "Weatherman": "https://weatherman.personalassistant.site"
  },
  "Auth0": {
    "Domain": "personalassistant-site.eu.auth0.com",
    "Audience": "https://gateway.personalassistant.site"
  },
  "Serilog": {
    "MinimumLevel": "Error",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "storage/logs/gateway.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  }
}