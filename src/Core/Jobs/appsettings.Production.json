{
  "DbBackup": {
    "AzureStorageContainerName": "db-backup",
    "BackupsPath": "../backups/"
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "../Logs/jobs_log.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  }
}